{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predeploy": "next build",
    "deploy": "aws s3 sync out/ s3://$(grep BUCKET .env.local | cut -d '=' -f2)/",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id $(grep DISTRIBUTION_ID .env.local | cut -d '=' -f2)  --paths '/*'"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^16.2.7",
    "@contentful/rich-text-react-renderer": "^15.21.2",
    "contentful": "^10.11.7",
    "next": "14.2.3",
    "prismjs": "^1.29.0",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "20.14.7",
    "@types/react": "18.3.3",
    "typescript": "5.5.2"
  }
}
